{
  "name": "gldash",
  "version": "0.0.0-sr",
  "description": "GitLab Jobs dashboard",
  "main": "",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "now-build": "parcel build src/index.html",
    "release": "npx semantic-release@beta",
    "format": "npx prettier --write **/*.{ts,tsx,md,json,html}"
  },
  "keywords": [
    "gitlab",
    "dashboard",
    "jobs",
    "pipelines",
    "monitoring",
    "tv",
    "control",
    "panel"
  ],
  "author": "Ricardo Casares (https://analogic.al)",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "s-ago": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.10.1",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "homepage": "https://github.com/ricardocasares/gldash#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardocasares/gldash.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run prettier",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/ricardocasares/gldash/issues"
  }
}
